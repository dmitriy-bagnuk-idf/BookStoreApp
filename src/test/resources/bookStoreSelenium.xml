<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Book Store tests with Selenium" thread-count="1" parallel="tests">
    <listeners>
        <listener class-name="testNgUtils.InvokedMethodListener"/>
    </listeners>

    <test name="User login test" enabled="true">
        <classes>
            <class name="seleniumTests.SeleniumLoginTest"/>
        </classes>
    </test>

    <test name="Compare lists of book titles test" enabled="true">
        <classes>
            <class name="seleniumTests.SeleniumTitlesTest"/>
        </classes>
    </test>

</suite>